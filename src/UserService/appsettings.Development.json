{
  "ConnectionStrings": {
    "SQLConnectionString": "server=localhost:5432;database=user_development_db;uid=postgres;password=postgres;"
  },
  "TokenSettings": {
    "AccessTokenLifetimeInMinutes": "2880",
    "RefreshTokenLifetimeInMinutes": "10080",
    "TokenIssuer": "Develop.ScoreWorker.AuthService",
    "TokenAudience": "Develop.ScoreWorker.SummaryService"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://[::]:8040"
      }
    },
    "Limits": {
      "KeepAliveTimeout": 90000
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:o} [{Level:u3}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}